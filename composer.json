{
	"name": "edde-framework/edde",
	"homepage": "https://edde-framework.org",
	"license": "Apache-2.0",
	"type": "library",
	"description": "The Ultimate PHP Framework",
	"keywords": [
		"php",
		"framework"
	],
	"authors": [
		{
			"name": "Marek Hanzal",
			"email": "marek.hanzal@x32.cz"
		}
	],
	"config": {
		"optimize-autoloader": true,
		"vendor-dir": "lib"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=7.2",
		"ext-pcre": "*",
		"ext-PDO": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-pcntl": "*",
		"ext-posix": "*",
		"roave/security-advisories": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "*"
	}
}
